{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@constants": ["src/constants"],
      "@constants/*": ["src/constants/*"],
      "@navigation": ["src/navigation"],
      "@navigation/*": ["src/navigation/*"],
      "@primitives": ["src/primitives"],
      "@primitives/*": ["src/primitives/*"],
      "@screens": ["src/screens"],
      "@screens/*": ["src/screens/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@assets": ["assets"],
      "@assets/*": ["assets/*"],
      "@context": ["src/context"],
      "@context/*": ["src/context/*"]
    },
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  }
}
